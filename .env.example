# PostgreSQL (compose default values are set via docker-compose.yml)
PGHOST=localhost
PGPORT=5432
PGDATABASE=shiv_accounts
PGUSER=postgres
PGPASSWORD=postgres
# Or use a single DATABASE_URL instead of the above
# DATABASE_URL=postgresql://postgres:postgres@localhost:5432/shiv_accounts

# App
PING_MESSAGE=pong

# External APIs (place your API keys here if needed)
# GST_HSN_API has no key; proxy endpoint is /api/hsn
